
public class Ejemplo02 {

	public static void main(String[] args) {

		// Declaramos la variable nombre, de tipo String (se puede guardar en ella
		// String, es decir, un texto).
		String nombre;
		
		// Asignamos un valor inicial (inicializamos) a la variable nombre.
		nombre = "Carlos";
		
		// Ahora podemos usar la variable donde queramos:
		System.out.println("Tu nombre es " + nombre);
		

	}

}

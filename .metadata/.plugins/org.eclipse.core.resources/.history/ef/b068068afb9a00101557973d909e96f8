
public class Ejemplo12 {
	
	public static void main(String[] args) {
		
		System.out.println("Esto es una frase que sigue");
		System.out.println("en la línea de abajo.");
		
		System.out.print("Esto es otra frase que sigue ");
		System.out.println("en la misma línea.");
		
		String frase1 = "Estamos en septiembre";
		String frase2 = "y empieza a refrescar.";
		
		String concatenacion = frase1 + frase2;
		
		
		
		
	}

}

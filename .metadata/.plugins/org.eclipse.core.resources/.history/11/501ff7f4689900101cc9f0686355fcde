
public class Ejemplo04 {

	public static void main(String[] args) {

		// Variable booleana que indica si a una compra se le aplicará descuento (true)
		// o no (false).
		boolean descuentoAplicable = true;

		// Variable booleana que indica si un coche ha pasado su revisión de ITV (true)
		// o no (false).
		boolean itvPasada = false;

		
		// Variable float que guardan números decimales.
		float estatura = 1.70F;
		
		// Variable double para guardar un número decimal más grande.
		
		
	}

}
